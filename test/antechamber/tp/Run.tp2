#!/bin/csh -f

# "The Mulliken charges were generated by mopac in amber10; 
#  you may get different result if different QM program is applied"

../../../bin/antechamber -i tp.pdb -fi pdb -at gaff2 -o tp2.mol2 -fo mol2 \
   -c bcc >& antechamber2.out || goto error

../../../bin/parmchk2 -s 2 -i tp2.mol2 -f mol2 -o frcmod2 || goto error
../../../bin/parmchk2 -s 2 -i tp2.mol2 -f mol2 -o frcmod2.full -a Y || goto error


../../dacdif -r 1.e-3 tp.mol2.save tp2.mol2
../../dacdif frcmod.save frcmod2
../../dacdif frcmod2.full.save frcmod2.full

/bin/rm -f ANTE* ATOMTYPE.INF BCCTYPE.INF for* FOR* NEWPDB.PDB PREP.INF \
      prmcrd divcon.in divcon.rst divcon.dmx leap.log leap.out
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)
